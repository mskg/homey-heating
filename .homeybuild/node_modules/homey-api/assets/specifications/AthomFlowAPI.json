{
  "host": "flow-api.athom.com",
  "basePath": "/api",
  "operations": {
    "shareFlow": {
      "path": "/flow",
      "method": "post",
      "parameters": {
        "data": {
          "in": "body",
          "type": "object",
          "required": true,
          "unpack": true
        }
      }
    },
    "getFlow": {
      "path": "/flow/{flowId}",
      "method": "get",
      "parameters": {
        "flowId": {
          "in": "path",
          "type": "string",
          "required": true
        }
      }
    }
  }
}